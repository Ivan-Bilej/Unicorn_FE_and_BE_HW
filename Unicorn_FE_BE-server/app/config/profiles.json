{
  "{asid}": {
    "profileList": ["AsidAuthorities","AsidExecutives", "AsidLicenseOwner","Public"],
    "useCaseMap": {}
  },
  "*": {
    "profileList": ["Authorities","Executives", "Readers", "AwidLicenseOwner","Public","Authenticated","AwidInitiator","AwidLicenceOwner"],
    "useCaseMap": {
      "sys/uuAppWorkspace/init": {
        "sysStateList": ["created","assigned"],
        "profileList": ["AwidLicenseOwner","AwidInitiator"]
      },
      "sys/uuAppWorkspace/load": {
        "sysStateList": ["active"],
        "profileList": ["Authenticated", "Authorities", "Executives", "Readers"]
      },
      "sys/uuAppWorkspace/loadBasicData": {
        "sysStateList": ["active"],
        "profileList": ["Public"]
      },
      "defaultUve": {
        "profileList": ["Public"]
      },
      "sys/uuAppWorkspace/initUve": {
        "sysStateList": ["created"],
        "profileList": ["Public","AwidLicenseOwner"]
      },
      "helloWorld": {
        "sysStateList": ["active"],
        "profileList": ["Public","AwidLicenseOwner","AwidInitiator"]
      },
      "shoppingList/create": {
        "sysStateList": ["active"],
        "profileList": ["Executives", "Readers"]
      },
      "shoppingList/list": {
        "sysStateList": ["active"],
        "profileList": ["Public", "Executives", "Readers"]
      }
    }
  }
}